{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)\\.html",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=60, stale-while-revalidate=600"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        { "source": "/quiz", "destination": "/quiz.html" },
        { "source": "/dados", "destination": "/dados.html" },
        { "source": "/endereco", "destination": "/endereco.html" },
        { "source": "/processando", "destination": "/processando.html" },
        { "source": "/sucesso", "destination": "/sucesso.html" },
        { "source": "/checkout", "destination": "/checkout.html" },
        { "source": "/orderbump", "destination": "/orderbump.html" },
        { "source": "/pix", "destination": "/pix.html" }
    ]
}
